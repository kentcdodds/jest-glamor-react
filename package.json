{
  "name": "jest-glamor-react",
  "version": "0.0.0-semantically-released",
  "description": "Jest utilities for Glamor and React",
  "main": "./index.js",
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "lint-staged && opt --in pre-commit --exec \"npm start validate\""
  },
  "files": [
    "dist",
    "index.js"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "css": "^2.2.1",
    "glamor": "^2.20.24",
    "jest-diff": "^21.0.2",
    "jest-snapshot": "^21.0.2",
    "strip-ansi": "^3.0.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.0.1",
    "babel-cli": "^6.23.0",
    "babel-jest": "^21.0.2",
    "babel-preset-env": "^1.2.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.23.0",
    "codecov": "^2.1.0",
    "commitizen": "^2.9.6",
    "cxs": "^3.0.4",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.17.0",
    "eslint-config-kentcdodds": "^12.0.0",
    "husky": "^0.13.2",
    "jest-cli": "^21.0.2",
    "lint-staged": "^3.3.1",
    "nps": "^5.0.3",
    "nps-utils": "^1.1.2",
    "opt-cli": "^1.5.1",
    "prettier-eslint-cli": "^3.1.2",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "semantic-release": "^6.3.2",
    "validate-commit-msg": "^2.11.1"
  },
  "peerDependencies": {
    "glamor": "^2 || ^3"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/jest",
      "kentcdodds/prettier",
      "kentcdodds/react"
    ],
    "rules": {
      "func-style": "off"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/jest-glamor-react.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/jest-glamor-react/issues"
  },
  "homepage": "https://github.com/kentcdodds/jest-glamor-react#readme"
}
